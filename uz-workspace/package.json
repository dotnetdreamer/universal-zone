{
  "name": "uz-workspace",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run watch:uz\" \"npm run watch:ionic\"",
    "build:ionic": "ng build ngx-ionic-zone --configuration production",
    "watch:ionic": "ng build ngx-ionic-zone --configuration development --watch",
    "build:uz": "ng build ngx-universal-zone --configuration production",
    "watch:uz": "ng build ngx-universal-zone --configuration development --watch",
    "build": "npm run build:all",
    "build:all": "npm run build:uz && npm run build:ionic",
    "test": "ng test",
    "clean": "rimraf node_modules"
  },
  "private": true,
  "dependencies": {
