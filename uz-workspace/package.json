    "@capacitor/network": "^7.0.2",
    "@ionic/angular": "^8.7.3",
    "core-js": "^3.44.0",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "ionicons": "^7.4.0",
